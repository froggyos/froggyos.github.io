{
    "scopeName": "source.froggytypeannotation",
    "patterns": [
        { "include": "#state" },
        { "include": "#types" }
    ],
    "repository": {
        "state": {
            "patterns": [
                {
                    "name": "temp.froggytypeannotation",
                    "match": "\\btemp\\b"
                },
                {
                    "name": "mut.froggytypeannotation",
                    "match": "\\bmut\\b"
                },
                {
                    "name": "const.froggytypeannotation",
                    "match": "\\bconst\\b"
                },
                {
                    "name": "ref.froggytypeannotation",
                    "match": "\\bref\\b"
                }
            ]
        },
        "types": {
            "patterns": [
                {
                    "name": "type.froggytypeannotation",
                    "match": "(?<=<)(array|number|string)(?=>)"
                },
                {
                    "name": "var.type.froggytypeannotation",
                    "match": "(?<=<)(variable)(?=>)"
                }
            ]
        }
    }
}